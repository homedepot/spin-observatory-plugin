{
  "name": "spin-observatory-plugin-deck",
  "version": "1.0.0",
  "private": true,
  "module": "build/dist/index.js",
  "scripts": {
    "clean": "npx shx rm -rf build",
    "develop": "npm run clean && run-p watch proxy",
    "build": "npm run clean && NODE_ENV=production rollup -c",
    "lint": "eslint --ext js,jsx,ts,tsx src",
    "postinstall": "check-plugin && check-peer-dependencies || true",
    "prepare": "husky-install",
    "prettier": "prettier --write 'src/**/*.{js,jsx,ts,tsx,html,css,less,json}'",
    "proxy": "dev-proxy",
    "watch": "rollup -c -w --no-watch.clearScreen",
    "modules": "echo 'yarn modules'"
  },
  "devProxy": {
    "host": "https://dev.spinnaker.homedepot.com/"
  },
  "dependencies": {
    "@date-io/date-fns": "1",
    "@material-ui/core": "^4.12.4",
    "@material-ui/icons": "^4.11.3",
    "@material-ui/lab": "^4.0.0-alpha.61",
    "@material-ui/pickers": "^3.3.10",
    "@rollup/plugin-commonjs": "18.0.0",
    "@rollup/plugin-typescript": "8.2.1",
    "@rollup/plugin-url": "6.0.0",
    "@spinnaker/core": "0.23.0",
    "@spinnaker/pluginsdk": "0.3.0",
    "@spinnaker/pluginsdk-peerdeps": "0.15.0",
    "@spinnaker/presentation": "0.2.1",
    "@types/react-dom": "18.0.11",
    "@uirouter/core": "6.0.4",
    "@uirouter/react": "1.0.2",
    "graphql": "16.6.0",
    "lodash-es": "4.17.21",
    "prettier": "2.1.2",
    "prop-types": "15.6.1",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-select": "1.2.1",
    "react-virtualized-select": "^3.1.3",
    "rollup": "2.45.2",
    "rxjs": "6.6.7",
    "sass": "^1.62.1",
    "typescript": "4.9.5"
  },
  "devDependencies": {
    "@rollup/plugin-json": "4.1.0",
    "@rollup/plugin-node-resolve": "11.2.1",
    "@rollup/plugin-replace": "2.4.2",
    "@types/react": "16.8.25",
    "@typescript-eslint/eslint-plugin": "4.4.0",
    "@typescript-eslint/parser": "4.4.0",
    "bufferutil": "4.0.2",
    "eslint": "7.10.0",
    "eslint-config-prettier": "6.12.0",
    "eslint-plugin-react-hooks": "4.1.2",
    "npm-run-all": "4.1.5",
    "pretty-quick": "3.1.3",
    "rollup-plugin-external-globals": "0.6.1",
    "rollup-plugin-terser": "7.0.2",
    "rollup-plugin-visualizer": "5.4.1",
    "shx": "0.3.3",
    "utf-8-validate": "5.0.3"
  },
  "files": [
    "build/dist"
  ]
}
